{
  "name": "stormjs",
  "version": "0.1.0",
  "description": "StormLib for Javascript, powered by Emscripten",
  "main": "dist/package/index.js",
  "files": [
    "dist/package"
  ],
  "scripts": {
    "build": "npm run build:stormlib && npm run build:webpack",
    "build:stormlib": "node src/build/stormlib.js",
    "build:package": "cpx \"build/stormlib.*.js\" dist/package && babel src/lib --out-dir dist/package",
    "build:standalone": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": "github:wowserhq/wowser-math",
  "keywords": [
    "mpq",
    "stormlib",
    "wow",
    "wowser"
  ],
  "author": "Wowser Contributors",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.37",
    "@babel/core": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "cpx": "^1.5.0",
    "fs-extra": "^5.0.0",
    "webpack": "^3.10.0"
  }
}
